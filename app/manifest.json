{
    "name": "XOR",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "XOR realizes XOR encryption/decryption method",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "scripts": [
            "scripts/bootstrap.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "XOR",
        "default_popup": "popup.html"
    }
}